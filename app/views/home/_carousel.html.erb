<div id="carousel-example-generic" class="carousel slide">
  <!-- Indicators -->
  <div class="carousel-words"></div>
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    <li data-target="#carousel-example-generic" data-slide-to="3"></li>
    <li data-target="#carousel-example-generic" data-slide-to="4"></li>
    <li data-target="#carousel-example-generic" data-slide-to="5"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active" words="words 1">
      <%=image_tag "background-1b.jpg"%>
    </div>
    <div class="item" words="words 2">
      <%=image_tag "background-2b.jpg"%>
    </div>
    <div class="item" words="words 3">
      <%=image_tag "background-3b.jpg"%>
    </div>
    <div class="item" words="words 4">
      <%=image_tag "background-4b.jpg"%>
    </div>
    <div class="item" words="words 5">
      <%=image_tag "background-5b.jpg"%>
    </div>
    <div class="item" words="words 6">
      <%=image_tag "background-6b.jpg"%>
    </div>
  </div>


</div>
